# Yellow SDK Configuration
# Copy this file to .env and fill in your values

# Your Ethereum wallet address
WALLET_ADDRESS=0xYourWalletAddress

# Your private key (NEVER commit this to version control)
PRIVATE_KEY=0xYourPrivateKey

# Your application name (optional, defaults to 'yellow-test_AGR')
APP_NAME=yellow-test_AGR

# Yellow Network endpoint (optional, defaults to ClearNet)
YELLOW_WS_ENDPOINT=wss://clearnet.yellow.com/ws

# Session expiration time in seconds (optional, defaults to 3600)
SESSION_EXPIRY=3600
